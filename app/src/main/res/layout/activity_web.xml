<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fanfan.mynewsapp.Activity.WebActivity">
   <RelativeLayout
       android:background="#99000a"
       android:id="@+id/rl_ayout"
       android:layout_width="match_parent"
       android:layout_height="50dp">
       <ImageButton
           android:background="#900"
           android:id="@+id/ibtn_shoucang"
           android:layout_marginRight="25dp"
           android:src="@drawable/news_menu"
           android:layout_alignParentRight="true"
           android:layout_width="45dp"
           android:layout_height="45dp"/>
       <TextView
           android:textSize="40dp"
           android:layout_centerHorizontal="true"
           android:text="资讯"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"/>
   </RelativeLayout>
    <WebView
        android:layout_below="@+id/rl_ayout"
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </WebView>
</RelativeLayout>
